{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "SiteQuadra.Middleware.SecurityLoggingMiddleware": "Information",
      "SiteQuadra.Middleware.AdminAuthMiddleware": "Warning"
    },
    "Console": {
      "IncludeScopes": true,
      "TimestampFormat": "yyyy-MM-dd HH:mm:ss "
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=quadra.db"
  },
  "AllowedOrigins": [
    "https://seudominio.com",
    "https://www.seudominio.com"
  ],
  "AllowedHosts": "*"
}