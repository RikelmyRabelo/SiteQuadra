{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "SiteQuadra.Middleware.SecurityLoggingMiddleware": "Information"
    },
    "Console": {
      "IncludeScopes": false,
      "TimestampFormat": "yyyy-MM-dd HH:mm:ss "
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=/app/data/quadra.db"
  },
  "AllowedOrigins": [
    "https://*.railway.app",
    "https://*.up.railway.app"
  ],
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:8080"
      }
    }
  }
}